class Main {
    function void main() {
        var Ship ship;
        var int i, x, y;

        do MathExt.init();

        let x = 8;
        let y = 8;
        let i = 0;
        while (i < 360) {
            let ship = Ship.new(x, y, i);
            do ship.draw();
            let x = x + 16;
            let i = i + 15;
        }
        return;
    }
}
